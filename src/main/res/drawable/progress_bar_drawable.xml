<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="25dip" />
            <solid android:color="#B5B4B4"></solid>
            <stroke android:width="1dp" android:color="#756D6D" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="25dip" />
                <solid android:color="@color/actionbar_background"></solid>
                <stroke android:width="1dp" android:color="#756D6D" />
            </shape>
        </clip>
    </item>
</layer-list>